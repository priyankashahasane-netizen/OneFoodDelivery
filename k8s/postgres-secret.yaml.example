apiVersion: v1
kind: Secret
metadata:
  name: stack-delivery-secrets
type: Opaque
stringData:
  database-url: postgres://user:password@postgres-service:5432/stack_delivery
  redis-url: redis://redis-service:6379
  jwt-secret: your-jwt-secret-here
  optimoroute-api-key: your-optimoroute-api-key
  ipstack-api-key: your-ipstack-api-key
  admin-user: admin
  admin-pass: change-me-in-production


