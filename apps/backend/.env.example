NODE_ENV=development
PORT=3000

# Database (PostgreSQL)
DATABASE_URL=postgres://postgres:postgres@localhost:5432/stack_delivery
DATABASE_SCHEMA=public

# Redis
REDIS_URL=redis://localhost:6379

# Integrations
OPTIMOROUTE_BASE_URL=https://api.optimoroute.com/v1
OPTIMOROUTE_API_KEY=
IPSTACK_BASE_URL=http://api.ipstack.com
IPSTACK_API_KEY=
NOMINATIM_URL=https://nominatim.openstreetmap.org
OSM_TILES_URL=https://tile.openstreetmap.org/{z}/{x}/{y}.png

# Tracking Web Base
TRACKING_BASE_URL=http://localhost:3001/track

# CORS Configuration (comma-separated list of allowed origins)
# For production, specify exact origins: https://admin.example.com,https://app.example.com
# For development, use: *
CORS_ORIGINS=*

# Admin Auth (Password Login)
ADMIN_USER=admin
ADMIN_PASS=admin
ADMIN_ID=demo-admin-id

# Admin Auth (OTP Login) - Comma-separated list of admin phone numbers
# Example: ADMIN_PHONES=919876543210,919876543211
ADMIN_PHONES=

# JWT Configuration
JWT_SECRET=dev-secret-change-in-production
JWT_EXPIRATION=30d

# CubeOne API Configuration
CUBEONE_BASE_URL=https://apigw.cubeone.in
CUBEONE_LOGIN_URI=/v2/hybrid-auth/login



