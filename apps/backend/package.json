{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "NestJS backend for Stack Delivery platform",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:cov": "jest --config jest.config.cjs --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.ts\"",
    "schema:sync": "npm run typeorm schema:sync",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "typeorm": "node --loader ts-node/esm ./node_modules/typeorm/cli.js -d src/config/typeorm.config.ts",
    "seed": "ts-node --esm src/database/seeds/run-seed.ts",
    "seed-all": "ts-node --esm src/database/seeds/seed-all-tables.ts",
    "create-notifications-table": "node dist/database/seeds/create-notifications-table.js",
    "seed-demo-complete": "node --loader ts-node/esm src/database/seeds/seed-demo-complete.ts",
    "add-mock-orders": "node --loader ts-node/esm src/database/seeds/add-mock-orders.ts",
    "reassign-orders-to-demo": "node --loader ts-node/esm src/database/seeds/reassign-orders-to-demo.ts",
    "create-demo-account": "node --loader ts-node/esm src/database/seeds/create-demo-account.ts",
    "consolidate-demo-account": "node --loader ts-node/esm src/database/seeds/consolidate-demo-account.ts",
    "add-active-order-demo": "node --loader ts-node/esm src/database/seeds/add-active-order-demo.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.5",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.4.5",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.5",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "axios": "^1.7.5",
    "bcrypt": "^6.0.0",
    "cache-manager": "^5.4.1",
    "cache-manager-ioredis-yet": "^2.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "joi": "^18.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.4.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/ws": "^8.5.10",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
